{
    "name": "Image Grabber",
    "description": "Extract all images from current web page",
    "version": "1.3",
    "manifest_version": 3,
    "action": {
        "default_popup":"popup.html"
    },
    "background": {
        "service_worker":"background.js"
      },
    "permissions": ["scripting", "activeTab", "contextMenus"]
}